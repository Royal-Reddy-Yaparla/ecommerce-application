[Unit]
Description = Dispatch Service
[Service]
User=roboshop
// highlight-start
Environment=AMQP_HOST={{ RABBITMQ_HOST }}
// highlight-end
Environment=AMQP_USER={{ AMQP_USER }}
Environment=AMQP_PASS={{ AMQP_PASS }}
ExecStart=/app/dispatch
SyslogIdentifier=dispatch

[Install]
WantedBy=multi-user.target